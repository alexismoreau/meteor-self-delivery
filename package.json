{
  "name": "hello",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "bcrypt": "^1.0.1",
    "fibers": "^1.0.15",
    "google-distance": "^1.0.1",
    "grunt": "^1.0.1",
    "meteor-node-stubs": "~0.2.0",
    "mocha": "^3.2.0",
    "time": "^0.11.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "meteor"
    ],
    "space": 2,
    "rules": {
      "max-len": ["error", 80, 2],
      "import/no-unresolved": ["error", { "ignore": ["^meteor/", "^/"] }],
      "import/no-extraneous-dependencies": [0, {"ignore": ["meteor"]}]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {"browsers": ["last 2 versions"]}
    }
  }
}
