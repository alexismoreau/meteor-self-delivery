{
  "name": "hello",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "google-distance": "^1.0.1",
    "grunt": "^1.0.1",
    "meteor-node-stubs": "~0.2.0"
  },
  "devDependencies": {
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node"
    ],
    "space": 2,
    "rules": {
      "max-len": [
        "error",
        80,
        2
      ],
      "import/no-unresolved": [
        2, { "ignore": ["^meteor/"] }
      ],
      "import/no-extraneous-dependencies": [
        0, { "ignore": ["meteor"] }
      ],
      "import/no-unassigned-import": [
        0, { "ignore": ["./main.html"] }
      ],
      "func-names": ["error", "as-needed"]
    },
    "new-cap": [
      "error",
      {
        "capIsNewExceptionpattern": "Router"
      }
    ]
  }
}
